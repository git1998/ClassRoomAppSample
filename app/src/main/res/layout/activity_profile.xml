<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ltd.akhbod.studentapp.ProfileActivity">

    <ltd.akhbod.studentapp.CustomViewPager
        android:id="@+id/profile_CustomviewPager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignBottom="@+id/profile_bottonNav"
        android:layout_alignParentStart="true">

    </ltd.akhbod.studentapp.CustomViewPager>

    <com.aurelhubert.ahbottomnavigation.AHBottomNavigation
        android:id="@+id/profile_bottonNav"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="bottom"
        android:layout_marginTop="510sp" />

</RelativeLayout>
